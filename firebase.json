{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
      "headers": [{
          "source": "**",
          "headers": [{
              "key": "Cache-control",
              "value": "no-cache, must-revalidate"
          }]
      }]
  }
}
